@article{li_molecular_2016,
	title = {Molecular {Dynamics} {Simulations} of {CO} $_{\textrm{2}}$ /{N} $_{\textrm{2}}$ {Separation} through {Two}-{Dimensional} {Graphene} {Oxide} {Membranes}},
	volume = {120},
	issn = {1932-7447, 1932-7455},
