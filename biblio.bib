@article{tang_wrinkle-_2018,
	title = {Wrinkle- and {Edge}-{Adsorption} of {Aromatic} {Compounds} on {Graphene} {Oxide} as {Revealed} by {Atomic} {Force} {Microscopy}, {Molecular} {Dynamics} {Simulation}, and {Density} {Functional} {Theory}},
